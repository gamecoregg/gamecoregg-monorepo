{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "baseUrl": "./",
    "rootDir": "./src",
    "outDir": "./dist",
    "paths": {
      "@gamecorelabs/nestjs-core": [
        "node_modules/@gamecorelabs/nestjs-core/dist"
      ],
      "@gamecorelabs/nestjs-core/*": [
        "node_modules/@gamecorelabs/nestjs-core/dist/*"
      ]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["test", "**/*.spec.ts", "node_modules"]
}
